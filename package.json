{
  "name": "my-medium",
  "version": "0.1.0",
  "description": "Static-first publishing prototype built with Eleventy for the internal Medium-style site.",
  "type": "module",
  "scripts": {
    "dev": "npx @11ty/eleventy --serve",
    "build": "npx @11ty/eleventy",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "npm run lint"
  },
  "keywords": [
    "eleventy",
    "jamstack",
    "publishing"
  ],
  "author": "Leco Conti",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LecoConti/my-medium.git"
