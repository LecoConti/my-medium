  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^5.0.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-toc-done-right": "^4.2.0",
    "markdown-it-prism": "^3.0.0",
    "minisearch": "^7.2.0",
    "reading-time": "^1.5.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "eslint": "^9.36.0",
    "prettier": "^3.6.2"
